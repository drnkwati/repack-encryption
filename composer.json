{
    "name": "drnkwati/repack-encryption",
    "description": "PHP 5.3 Encryption package.",
    "license": "MIT",
    "authors": [
    {
        "name": "Taylor Otwell",
        "email": "taylor@laravel.com"
    },
    {
        "name": "Divine Nkwati",
        "email": "drnkwati@gmail.com"
    }],
    "require":
    {
        "php": ">=5.3.3",
        "ext-json": "*",
        "ext-mbstring": "*",
        "ext-openssl": "*",
        "paragonie/random_compat": "<=2.0"
    },
    "autoload":
    {
        "psr-4":
        {
            "Repack\\Encryption\\": "src"
        }
    },
